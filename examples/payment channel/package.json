{
  "name": "Althea",
  "version": "0.0.1",
  "private": true,
  "scripts": {},
  "dependencies": {

  },
  "browserify": {

  },
  "devDependencies": {

  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-shadow": 0,
      "dot-notation": 0,
      "no-use-before-define": 0,
      "eqeqeq": 2,
      "semi": 0,
      "strict": 0,
      "curly": 2,
      "camelcase": 0,
      "handle-callback-err": 0,
      "no-unused-vars": 1,
      "no-constant-condition": 0,
      "no-unused-expressions": 0,
      "no-loop-func": 0,
      "quotes": [
        2,
        "single"
      ],
      "no-underscore-dangle": 0,
      "comma-dangle": 0
    }
  }
}
